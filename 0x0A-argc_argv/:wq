#include <stdio.h>
#include "main.h"

/*
 * main - a program that prints the number of arguments passed into it
 * @argc: number of argumments
 * @argv: arry of argumments
 * Return: 0 (Success)
 */
int main(int argc, char *argv[])
{
	(void) argv; /* ignore argv*/
	prinf("%d\n", argc - 1);
	return (0);
}
